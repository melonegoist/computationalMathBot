{
    "interval": {
        "regex": "\\[(-?\\d+\\.?\\d*)\\s+(-?\\d+\\.?\\d*)\\]",
        "description": "Паттерн для строк вида [a b], где a и b — числа (включая float и отрицательные)"
    },

    "accuracy": {
        "regex": "(0\\.\\d*[1-9]\\d*|0\\.[0-9]*[1-9])",
        "description": ""
    },

    "matrix": {
        "regex": "(?:\\s*-?\\d+\\.?\\d*(?:\\s+(?!$)|$))+(?:\n(?:\\s*-?\\d+\\.?\\d*(?:\\s+(?!$)|$))+)*",
        "description": ""
    },

    "equation": {
        "regex": "(?:[+-]?\\s*\\d*\\.?\\d+\\*?[xyz](?:\\*\\*\\d+)?)(?:\\s*[+-]\\s*(?:[+-]?\\s*\\d*\\.?\\d+\\*?[xyz](?:\\*\\*\\d+)?|\\d+\\.?\\d*))*",
        "description": ""
    }

}
